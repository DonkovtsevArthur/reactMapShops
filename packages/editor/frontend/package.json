	"description": "Edit goods in evotor",
	"main": "index.js",
	"lint-staged": {
		"src/**/*.{js,jsx,json,css}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"precommit": "lint-staged",
		"start": "NODE_ENV=development gulp dev",
		"build": "NODE_ENV='production' gulp build",
		"versionInc": "perl -i -pe 's/^((\\d+\\.)*)(\\d+)(.*)$/$1.($3+1).$4/e' < version.num version.num",
		"dockerbuild": "yarn versionInc && gulp build && sudo docker build -t lyulyaevma/nomenclature:$(cat version.num) . && sudo docker push lyulyaevma/nomenclature:$(cat version.num)",
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
	},
	"author": "kuchinsn",
	"license": "ISC",
